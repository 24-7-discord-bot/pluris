{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "max-len": "off",
        "no-underscore-dangle": "off",
        "consistent-return": "off",
        "no-param-reassign": "off",
        "no-console": "off",
        "global-require": "off",
        "import/no-dynamic-require": "off"
    }
}